      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
    </supports>
    <portlet-info>
      <title>Navigation Portlet</title>
      <short-title>Navigation</short-title>
    </portlet-info>
  </portlet>
  <portlet>
    <portlet-name>SiteMapPortlet</portlet-name>
    <portlet-class>org.gatein.portal.ui.sitemap.SiteMapPortlet</portlet-class>
